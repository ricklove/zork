{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "jsx": "react-native",
        "lib": [
            "dom",
            "esnext",
            "node"
        ],
        "moduleResolution": "node",
        "noEmit": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "strictNullChecks": true
    }
}